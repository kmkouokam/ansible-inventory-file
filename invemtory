# define variables at the host level
web01 ansible_host=10.0.19.20

web02 ansible_host=10.0.27.7

db01 ansible_host=10.0.0.163



[webservergrp]
web01
web02



[dbservergrp]
db01


#group of group


[dc_ohio:children]
webservergrp
dbservergrp

#define variables at group of group level
[dc_ohio:vars]

ansible_user=centos
ansible_ssh_private_key_file=vprofile-key.pem

